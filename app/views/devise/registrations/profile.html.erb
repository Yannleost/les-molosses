
<div class="container">
	<div class="row">
<h1> Welcome to your profile <%= current_user.first_name %> <%= current_user.last_name %> </h1>
	<div class="col-xs-6 ">
		<h3> Your booking(s)</h3>
			<% current_user.bookings.each do |booking| %>
			<ul>
			  <li> Booking_id <%= booking.id %> </li> 
			  <li> Chien book√© : <%= booking.dog.nickname %> </li> 
			  <li> Maitre : <%= booking.dog.user.email %> </li> 
			  <li> Start_date : <%= booking.start_date %> </li> 
			  <li> End_date : <%= booking.end_date %> </li> 
			  <li> <%= link_to "laisser une review",  new_dog_booking_review_path(booking.id, booking.dog.id), class: "btn btn-primary list-inline"%> </li> 
			</ul>
		 
		  <%end%>
	</div>

</div>
</div>